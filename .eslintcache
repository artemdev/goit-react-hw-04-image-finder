  const [isLoading, setIsLoading] = useState(false)\n  const [page, setPage] = useState(1)\n\n  useEffect(() => {\n    if(query) {\n      setIsLoading(true)\n      getImages(query)\n        .then(\n          images =>\n            !!images.length &&\n            setImages(images),\n        )\n        .finally(() => {\n          setIsLoading(false)\n        });\n    }\n  }, [query])\n\n  useEffect(() => {\n    if(query) {\n    setIsLoading(true)\n    getImages(query, page)\n    .then(newImages => {\n      setImages([...images, ...newImages]);\n      setIsLoading(false)\n    })\n    .then(() =>\n      window.scrollTo({\n        top: document.documentElement.scrollHeight,\n        behavior: 'smooth',\n      }),\n    )\n    .catch(console.log);\n    }\n  }, [page])\n\n  const loadMore = e => {\n    e.preventDefault();\n    setPage(prevPage => prevPage + 1);\n  };\n\n    return (\n      <section>\n        {isLoading ? \n           <Loader\n          type=\"Puff\"\n          color=\"#00BFFF\"\n          height={100}\n          width={100}\n          timeout={3000}\n        /> : images.length ? (\n          <section>\n            <ul className=\"ImageGallery\">\n              {images.map(image => (\n                <li key={shortid.generate()} className=\"ImageGalleryItem\">\n                  <GalleryItem image={image} />\n                </li>\n              ))}\n            </ul>\n            <button\n              className=\"Button\"\n              type=\"submit\"\n              onClick={loadMore}\n            >\n              Load more\n            </button>\n          </section>\n        ) : (\n          <h3>Please, enter the name of the image to find</h3>\n        )}\n      </section>\n    );\n}\n","/Users/artem/Desktop/GoIt/module3/homework-4/goit-react-hw-04-image-finder/src/App.js",[],"/Users/artem/Desktop/GoIt/module3/homework-4/goit-react-hw-04-image-finder/src/api/getImages.js",[],["49","50"],"/Users/artem/Desktop/GoIt/module3/homework-4/goit-react-hw-04-imag